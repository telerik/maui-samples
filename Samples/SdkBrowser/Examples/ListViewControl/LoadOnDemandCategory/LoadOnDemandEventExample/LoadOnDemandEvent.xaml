<?xml version="1.0" encoding="utf-8" ?>
<telerik:RadContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                        xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                        xmlns:telerik="http://schemas.telerik.com/2022/xaml/maui"
                        xmlns:local="clr-namespace:SDKBrowserMaui.Examples.ListViewControl.LoadOnDemandCategory.LoadOnDemandCommandExample"
                        x:Class="SDKBrowserMaui.Examples.ListViewControl.LoadOnDemandCategory.LoadOnDemandEventExample.LoadOnDemandEvent"
                        Margin="5">
    <telerik:RadContentView.Content>
        <Grid>
            <!--  >> listview-loadondemand-loadondemandeventauto-declaration  -->
            <telerik:RadListView x:Name="listView"
                                 IsLoadOnDemandEnabled="True"
                                 LoadOnDemand="ListView_LoadOnDemand"
                                 LoadOnDemandMode="Automatic"/>
            <!--  << listview-loadondemand-loadondemandeventauto-declaration  -->
        </Grid>
    </telerik:RadContentView.Content>
</telerik:RadContentView>